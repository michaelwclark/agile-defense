  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.55.0",
    "node-fetch": "^3.3.2",
    "vitest": "^3.2.4"
  }
